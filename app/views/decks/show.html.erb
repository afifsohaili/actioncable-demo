<div class="row">
  <div class="col s12">
    <div class="row">
      <div class="col s12">
        <h4><%= deck.title %></h4>
      </div>
    </div>

    <div class="row" id="slide-placeholder">
      <div class="col s12">
        <%= render "slide", slide: deck.first_slide %>
      </div>
    </div>

  </div>
</div>
<a class="right btn-floating btn-large waves-effect waves-light light-blue" id="next" data-deck-id="<%= deck.id %>">
  <i class="material-icons">add</i>
</a>
