<div class="row">
  <div class="col s12">
    <div class="row">
      <div class="col s12" id="slide-placeholder">
        <%= render "decks/demo/#{deck.current_slide_partial}" %>
      </div>
    </div>

  </div>
  <input type="hidden" value="<%= deck.current_slide_position %>" id="slide-id">
</div>
<a class="right btn-floating btn-large waves-effect waves-light light-blue" id="next" data-deck-id="<%= deck.id %>">
  <i class="material-icons">keyboard_arrow_right</i>
</a>
<a class="left btn-floating btn-large waves-effect waves-light light-blue" id="prev" data-deck-id="<%= deck.id %>">
  <i class="material-icons">keyboard_arrow_left</i>
</a>
